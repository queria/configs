[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    dfc = diff --cached
    lg = log -p
    lom = log master..origin/master
    lns = log --name-status

[core]
    autocrlf = input
    attributesfile = ~/.gitattributes

